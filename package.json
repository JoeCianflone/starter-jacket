{
   "private": true,
   "scripts": {
      "env:save": "gpg --symmetric --cipher-algo AES256 .env",
      "env:decrypt": "gpg  --decrypt --output .env-update .env.gpg",
      "lint:css": "yarn stylelint resources/_css/**/*.scss",
      "lint:js": "yarn run eslint 'resources/_js/**' -c .eslintrc.json",
      "check:static": "./vendor/bin/phpstan analyse --memory-limit=2G",
      "format:php": "php-cs-fixer fix --config=php-cs-fixer.dist app routes",
      "format:js": "yarn prettier --write resources/_js",
      "generate:deps": "yarn install --frozen-lockfile && composer install",
      "generate:routes": "php artisan ziggy:generate resources/_js/ziggy.js",
      "compile": "yarn run generate:routes",
      "build:dev": "yarn run compile && mix",
      "build:production": "yarn run compile && mix --production",
      "build:watch": "yarn run compile && mix watch"
   },
   "browserslist": [
      "last 3 version",
      "> 0.5%",
      "not dead",
      "not IE 11"
   ],
   "devDependencies": {
      "clean-webpack-plugin": "^4.0.0-alpha.0",
      "eslint": "^7.30.0",
      "eslint-config-prettier": "^8.3.0",
      "eslint-plugin-prettier": "^3.4.0",
      "eslint-plugin-vue": "^7.13.0",
      "laravel-mix": "^6.0.24",
      "laravel-mix-clean": "^0.1.0",
      "prettier": "^2.2.1",
      "stylelint": "^13.13.1",
      "stylelint-config-standard": "^22.0.0",
      "stylelint-declaration-strict-value": "^1.7.12",
      "stylelint-order": "^4.1.0",
      "stylelint-scss": "^3.19.0"
   },
   "dependencies": {
      "@babel/core": "^7.13.8",
      "@babel/plugin-syntax-dynamic-import": "^7.8.3",
      "@inertiajs/inertia": "^0.10.0",
      "@inertiajs/inertia-vue": "^0.7.1",
      "@inertiajs/progress": "^0.2.4",
      "babel-loader": "^8.2.2",
      "bootstrap-icons": "^1.4.0",
      "css-loader": "^5.1.1",
      "lodash": "^4.17.19",
      "normalize.css": "^8.0.1",
      "popper.js": "^1.16.1",
      "portal-vue": "^2.1.7",
      "postcss": "^8.1.14",
      "postcss-cli": "^8.3.1",
      "postcss-import": "^14.0.0",
      "postcss-loader": "^6.1.0",
      "postcss-purgecss-laravel": "^2.0.0",
      "postcss-sort-media-queries": "^3.4.3",
      "resolve-url-loader": "^4.0.0",
      "sass": "^1.32.8",
      "sass-loader": "^12.1.0",
      "style-loader": "^3.0.0",
      "vue": "^2.6.12",
      "vue-loader": "^15.9.6",
      "vue-stripe-elements-plus": "^1.0.1",
      "vue-template-compiler": "^2.6.12",
      "vuex": "^3.6.2",
      "webpack": "^5.24.3"
   },
   "resolutions": {
      "sindresorhus/is-svg": "^4.0.0"
   }
}
